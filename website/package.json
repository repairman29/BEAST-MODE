      "team-optimization",
      "knowledge-management",
      "plugin-marketplace",
      "integration-marketplace",
      "tool-discovery",
      "monetization-platform"
    ],
    "supportedLanguages": [
      "javascript",
      "typescript",
      "python",
      "java",
      "csharp",
      "go",
      "rust",
      "php"
    ],
    "enterpriseFeatures": [
      "multi-repo-support",
      "team-analytics",
      "enterprise-licensing",
      "custom-integrations",
      "dedicated-support"
    ]
  }
}
