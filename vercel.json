{
  "buildCommand": "cd website && npm run build",
  "outputDirectory": "website/.next",
  "installCommand": "cd website && npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
